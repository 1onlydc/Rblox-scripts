		end

		table.insert(messages, "â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•")
		CreateMultiLineTerminalMessage(GetCurrentTime(), "[CONFIG]", messages, Color3.fromRGB(150, 150, 255))
	end
end)

